    "exec": "bin/ts-unused-exports"
  },
  "bin": {
    "ts-unused-exports": "bin/ts-unused-exports"
  },
  "devDependencies": {
    "jasmine": "^2.4.1",
    "tslint": "^4.3.1",
    "typings": "^1.3.0"
