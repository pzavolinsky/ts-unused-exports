{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
      "**/*test.ts"
    ],
    "require": ["ts-node/register"],
    "reporter": [
        "html",
        "text",
        "text-summary"
    ],
    "statements": 90,
    "branches": 80,
    "functions": 100,
    "lines": 90,
    "watermarks": {
      "statements": [80, 100],
      "branches": [80, 100],
      "functions": [80, 100],
      "lines": [80, 100]
    }
}
